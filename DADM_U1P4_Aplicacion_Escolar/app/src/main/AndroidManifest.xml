<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dadm_u1p4_aplicacion_escolar">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/firebase_logo_logomark"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/firebase_logo_logomark"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/Theme.DADM_U1P4_Aplicacion_Escolar">
        <activity android:name=".SeleccionActivity"></activity>
        <activity android:name=".Horarios" />
        <activity android:name=".Perfil" />
        <activity android:name=".AvanceCurricular" />
        <activity android:name=".Registrar" />
        <activity android:name=".Calificaciones" />
        <activity android:name=".Login" />
        <activity android:name=".Dashboard" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>